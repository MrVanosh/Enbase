{
  "parser": "babel-eslint",
  "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
  ],
  "globals": {
      "window": true,
      "document": true
  },
  "settings": {
      "import/resolver": {
          "node": {
              "paths": ["./src"],
              "extensions": [".js"]
          }
      }
  },
  "rules": {
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "react/state-in-constructor": 0,
      "react/prop-types": 0,
      "no-shadow": 0
  }
}